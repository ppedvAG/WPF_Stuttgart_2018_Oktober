<Window x:Class="UserControl_WebsiteChecker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserControl_WebsiteChecker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <local:UrlPreview WrongURL="checker_WrongURL" x:Name="checker" Panel.ZIndex="0" />
        <Button VerticalAlignment="Bottom" Click="Button_Click" >Reverse</Button>
    </Grid>
</Window>
